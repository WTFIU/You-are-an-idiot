{
  "manifest_version": 3,
  "name": "Chaos Windows",
  "version": "1.0",
  "description": "Fun pop-up chaos after user consent.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
